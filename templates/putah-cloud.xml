<?xml version="1.0"?>
<Container version="2">
  <Name>putah-cloud</Name>
  <Repository>javiervarela/putah-cloud:latest</Repository>
  <Registry>https://hub.docker.com/r/javiervarela/putah-cloud/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>true</Privileged>
  <Support>https://github.com/javiervarela/putah-cloud</Support>
  <Project>https://github.com/javiervarela/putah-cloud</Project>
  <Overview>Putah cloud minimal docker for custom putah network resources.</Overview>
  <Category>Backup: Status:Beta</Category>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/javiervarela/docker-templates/master/templates/putah-cloud.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/javiervarela/docker-templates/master/templates/icons/putah-cloud.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>Putah cloud minimal docker for custom putah network resources.</Description>
  <Labels/>
    <Data>
    <Volume>
      <HostDir>/mnt/disks/torrents/docker/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/disks/torrents/docker/cache</HostDir>
      <ContainerDir>/cache</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/disks/torrents/test</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw,shared</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/disks/torrents/__SUBIENDO</HostDir>
      <ContainerDir>/local</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/disks/torrents/docker/cloud</HostDir>
      <ContainerDir>/cloud</ContainerDir>
      <Mode>rw,shared</Mode>
    </Volume>
  </Data>
  <!--<Config Name="Port" Target="80" Default="14900" Mode="tcp" Description="Port" Type="Port" Display="always" Required="false" Mask="false">14900</Config>-->
</Container>